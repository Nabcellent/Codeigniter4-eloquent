{
    "name": "nabcellent/codeigniter4-eloquent",
    "type": "library",
    "description": "The Illuminate Database package for CodeIgniter 4",
    "keywords": ["codegniter4", "codeigniter4-eloquent", "database", "orm", "sql"],
    "license": "MIT",
    "authors": [
        {
            "name": "nabcellent",
            "email": "nabcellent.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "codeigniter4/framework": "^4.0",
        "illuminate/database": "^8.64",
        "illuminate/pagination": "^8.64"
    },
    "autoload": {
        "psr-4": {
            "Fluent\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always -vvv"
    }
}
